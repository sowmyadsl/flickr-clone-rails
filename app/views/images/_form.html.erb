<%= render "layouts/errors", :object => @image %>

<%= form_for @image, url: user_image_path, html: { multipart: true } do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>
  <%= f.label :avatar %>
  <%= f.file_field :avatar %>
  <br>
  <%= f.submit %>
<% end %>
