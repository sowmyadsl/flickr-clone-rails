<h1><%= @user.name %></h1>

<h3>Images</h3>

<ul>
  <% @user.images.each do |image| %>
    <li><%= link_to image.title, user_image_path(@user, image) %></li>
  <% end %>
</ul>

<p><%= link_to "Edit", edit_user_path(@user) %></p>
<p><%= link_to "Delete", user_path(@user),
                         :data => {:confirm => "You sure?",
                                   :method => "delete"} %></p>
<p><%= link_to "Return to users", users_path %></p>
